# SERAI Development Environment Variables
# Copy this file to .env and update with your actual values

# Application Ports (Avoiding Dreamery Project conflicts)
NODE_ENV=development
PORT=4001
FRONTEND_PORT=4000
OPERATIONS_PORT=4002
STRIPE_WEBHOOK_PORT=4003
REACT_NATIVE_PORT=8082

# Database Configuration
DATABASE_URL=postgresql://serai_user:serai_password@localhost:5433/serai_dev
REDIS_URL=redis://localhost:6380
ELASTICSEARCH_URL=http://localhost:9201

# Authentication & Security
JWT_SECRET=serai_dev_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=7d
REFRESH_TOKEN_SECRET=serai_dev_refresh_secret_key_change_in_production
REFRESH_TOKEN_EXPIRES_IN=30d

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here
STRIPE_WEBHOOK_ENDPOINT=http://localhost:4003/webhooks/stripe

# Twilio SMS & WhatsApp
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890
WHATSAPP_BUSINESS_ACCOUNT_ID=your_whatsapp_business_account_id

# AI & Machine Learning
OPENAI_API_KEY=sk-your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here

# Vector Database (Pinecone/Weaviate)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
WEAVIATE_URL=http://localhost:8080
WEAVIATE_API_KEY=your_weaviate_api_key_here

# Google Services
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_EVENTS_API_KEY=your_google_events_api_key_here
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Firebase & Notifications
FIREBASE_PROJECT_ID=serai-dev
FIREBASE_PRIVATE_KEY_ID=your_firebase_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_firebase_private_key_here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@serai-dev.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_firebase_client_id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token

# OneSignal Push Notifications
ONESIGNAL_APP_ID=your_onesignal_app_id_here
ONESIGNAL_API_KEY=your_onesignal_api_key_here
ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key_here

# OTA Integrations
BOOKING_COM_API_KEY=your_booking_com_api_key_here
AGODA_API_KEY=your_agoda_api_key_here
AIRBNB_API_KEY=your_airbnb_api_key_here
GOIBIBO_API_KEY=your_goibibo_api_key_here
MAKEMYTRIP_API_KEY=your_makemytrip_api_key_here

# Analytics & Monitoring
MIXPANEL_PROJECT_TOKEN=your_mixpanel_project_token_here
GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
HUBSPOT_API_KEY=your_hubspot_api_key_here
SENTRY_DSN=your_sentry_dsn_here

# Email Services
SENDGRID_API_KEY=your_sendgrid_api_key_here
SENDGRID_FROM_EMAIL=noreply@serai.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_smtp_user_here
SMTP_PASS=your_smtp_password_here

# File Storage
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=serai-dev-uploads
CLOUDFLARE_R2_ACCESS_KEY_ID=your_r2_access_key_id_here
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your_r2_secret_access_key_here
CLOUDFLARE_R2_BUCKET_NAME=serai-dev-uploads

# AI Agent Ports
CONCIERGE_AGENT_PORT=4004
REVENUE_AGENT_PORT=4005
REPUTATION_AGENT_PORT=4006
HOUSEKEEPING_AGENT_PORT=4007
CHECKIN_AGENT_PORT=4008
MAINTENANCE_AGENT_PORT=4009
UPSELL_AGENT_PORT=4010
INSIGHTS_AGENT_PORT=4011
LISTING_BOT_PORT=4012

# Development URLs
FRONTEND_URL=http://localhost:4000
BACKEND_URL=http://localhost:4001
OPERATIONS_URL=http://localhost:4002
WEBHOOK_URL=http://localhost:4003
CUSTOMER_APP_URL=http://localhost:8082

# CORS Configuration
CORS_ORIGIN=http://localhost:4000,http://localhost:4002,http://localhost:8082
CORS_CREDENTIALS=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug
LOG_FORMAT=combined

# Feature Flags
ENABLE_AI_AGENTS=true
ENABLE_OTA_SYNC=true
ENABLE_DYNAMIC_PRICING=true
ENABLE_CONTACTLESS_CHECKIN=true
ENABLE_UPSELL_RECOMMENDATIONS=true

# Security
BCRYPT_ROUNDS=12
SESSION_SECRET=serai_dev_session_secret_change_in_production
COOKIE_SECURE=false
COOKIE_HTTP_ONLY=true
COOKIE_SAME_SITE=lax

# Database Pool Configuration
DB_POOL_MIN=2
DB_POOL_MAX=10
DB_POOL_IDLE_TIMEOUT=30000
DB_POOL_ACQUIRE_TIMEOUT=60000

# Redis Configuration
REDIS_PASSWORD=serai_redis_password
REDIS_DB=0
REDIS_FAMILY=4

# Elasticsearch Configuration
ELASTICSEARCH_USERNAME=
ELASTICSEARCH_PASSWORD=
ELASTICSEARCH_INDEX_PREFIX=serai_dev_

# Webhook Configuration
WEBHOOK_TIMEOUT=30000
WEBHOOK_RETRY_ATTEMPTS=3
WEBHOOK_RETRY_DELAY=1000

# AI Model Configuration
DEFAULT_LLM_MODEL=gpt-4o
FALLBACK_LLM_MODEL=claude-3-sonnet
EMBEDDING_MODEL=text-embedding-3-large
MAX_TOKENS=4000
TEMPERATURE=0.7

# Pricing Engine Configuration
PRICING_UPDATE_INTERVAL=3600000
PRICING_MIN_FACTOR=0.5
PRICING_MAX_FACTOR=3.0
PRICING_DEFAULT_FACTOR=1.0

# Notification Configuration
NOTIFICATION_BATCH_SIZE=100
NOTIFICATION_DELAY_MS=1000
NOTIFICATION_MAX_RETRIES=3

# File Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,application/pdf
UPLOAD_PATH=./uploads

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
CACHE_UPDATE_INTERVAL=300000

# Monitoring Configuration
HEALTH_CHECK_INTERVAL=30000
METRICS_COLLECTION_INTERVAL=60000
ALERT_THRESHOLD_CPU=80
ALERT_THRESHOLD_MEMORY=85
ALERT_THRESHOLD_DISK=90
